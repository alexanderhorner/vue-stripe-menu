{
  "name": "vue-stripe-menu",
  "version": "2.1.0",
  "description": "Creating a navigation menu with animations like on Stripe",
  "keywords": [
    "vue",
    "stripe",
    "dropdown",
    "menu",
    "navigation",
    "animations",
    "component",
    "ui components",
    "stripe-elements"
  ],
  "author": "Alexey Khrushch <alexeykhr@outlook.com>",
  "scripts": {
    "build": "vue-cli-service build --dest docs ./demo/main.js",
    "lint": "vue-cli-service lint src demo --no-fix",
    "build:lib": "vue-cli-service build --target lib --name vue-stripe-menu ./src/main.js",
    "dev": "vue-cli-service serve ./demo/main.js",
    "lint:fix": "vue-cli-service lint src demo",
    "pub": "git push --follow-tags origin HEAD && npm publish",
    "rel": "npm run lint:fix && npm run test && npm run build:lib && standard-version --skip.changelog --skip.commit --skip.tag && npm run build && git add -A && standard-version --commit-all --skip.bump",
    "test": "jest"
  },
  "main": "./dist/vue-stripe-menu.umd.min.js",
  "dependencies": {
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.9",
    "@vue/test-utils": "^2.0.0-rc.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.8.0",
    "highlight.js": "^10.7.1",
    "jest": "^26.6.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.37.5",
    "sass-loader": "^10.1.1",
    "sinon": "^10.0.0",
    "vue-jest": "^5.0.0-alpha.8"
  },
  "bugs": {
    "url": "https://github.com/Alexeykhr/vue-stripe-menu/issues"
  },
  "homepage": "https://github.com/Alexeykhr/vue-stripe-menu#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexeykhr/vue-stripe-menu.git"
  }
}
